[user]
	name = shuhei
	email = 72839906+oleax21@users.noreply.github.com
[core]
	editor = code --wait
[alias]
	ad = add
	st = status
	br = branch
	brm = branch -m
	brd = branch -d
	brdd = branch -D
	ch = checkout
	sw = switch
	swc = switch -c
	r = reset
	rs = restore
	rb = rebase
	cm = commit
	cmm = commit -m
	p = push
	f = fetch
	mg = merge --no-ff
	mgff = merge --ff
	ss = stash
	ssl = stash list
	ssa = stash apply
	ssd = stash drop
	ssc = stash clear
	lg1 = log -1
	lg2 = log -2
	lg3 = log -3
	lgone = log --oneline
	lgg = log --graph --pretty=format:'%C(auto)%h%Creset -%C(auto)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	firstcom = commit --allow-empty -m \"Initial commit\"
[merge]
	ff = false
[pull]
	rebase = true
